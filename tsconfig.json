{
  "compilerOptions": {
    "target": "ES2020",
    "module": "ES2015",
    "moduleResolution": "node",
    "allowSyntheticDefaultImports": true,
    "outDir": "./lib",
    "rootDir": "./src",
    "strict": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "noImplicitReturns": true
  },
  "typedocOptions": {
    "out": "docs",
    "name": "@jsq/seq",
    "exclude": "**/*.+(spec|fixture).ts",
    "excludePrivate": true
  }
}
