{
  "name": "@jsq/seq",
  "version": "0.4.3",
  "description": "Lazy sequence operators for JavaScript.",
  "main": "lib/index",
  "module": "lib/index.mjs",
  "types": "lib/index.d.ts",
  "engines": {
    "node": ">=4"
  },
  "scripts": {
    "build": "tsc && rollup -c ./rollup.config.js",
    "docs": "typedoc src",
    "prepublishOnly": "npm run build",
    "pretest": "tsc -p tsconfig.test.json",
    "release": "standard-version",
    "test": "tape lib/*.spec.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jeskew/seq.git"
  },
  "keywords": [
    "asynciterator",
    "seq"
  ],
  "author": "Jonathan Eskew <jonathan@jeskew.net>",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/jeskew/seq/issues"
  },
  "homepage": "https://github.com/jeskew/seq#readme",
  "devDependencies": {
    "@types/tape": "^4.2.31",
    "rollup": "^1.0",
    "rollup-plugin-typescript": "^1.0",
    "standard-version": "^8.0",
    "tape": "^4.11.0",
    "typedoc": "^0.15.0",
    "typescript": "^3.5"
  },
  "dependencies": {
    "tslib": "^1.8.1"
  },
  "nyc": {
    "exclude": [
      "**/*.spec.js",
      "**/*.fixture.js"
    ]
  }
}
